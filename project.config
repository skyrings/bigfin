[project]
	description = BigFin is a SkyRing provider which implements the provider specification for Ceph Storage
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Nishanth Thomas (nthomas-redhat)
	label-Code-Review = -2..+2 group skyring
	label-Verified = -1..+1 group Registered Users
	label-Verified = -1..+1 group skyring
[access "refs/for/refs/*"]
	read = group user/Nishanth Thomas (nthomas-redhat)
	push = group user/Nishanth Thomas (nthomas-redhat)
	label-Code-Review = -2..+2 group skyring
	label-Code-Review = -2..+2 group user/Nishanth Thomas (nthomas-redhat)
	label-Verified = -1..+1 group Registered Users
	label-Verified = -1..+1 group skyring
	label-Verified = -1..+1 group user/Nishanth Thomas (nthomas-redhat)
	rebase = group user/Nishanth Thomas (nthomas-redhat)
	removeReviewer = group user/Nishanth Thomas (nthomas-redhat)
	submit = group skyring
	submit = group user/Nishanth Thomas (nthomas-redhat)
[access "refs/heads/*"]
	read = group user/Nishanth Thomas (nthomas-redhat)
	create = group user/Nishanth Thomas (nthomas-redhat)
	push = +force group user/Nishanth Thomas (nthomas-redhat)
	pushMerge = group user/Nishanth Thomas (nthomas-redhat)
	label-Code-Review = -2..+2 group skyring
	submit = group skyring
[access "refs/tags/*"]
	push = group skyring
	push = +force group user/Nishanth Thomas (nthomas-redhat)
